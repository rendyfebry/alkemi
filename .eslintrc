{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	},
	"extends": "airbnb",
	"rules": {
		"quotes": [
			2,
			"single"
		],
		"indent": [
			2,
			"tab"
		],
		"semi": [
			2,
			"never"
		],
		"no-tabs": 0,
		"react/jsx-indent": [
			2,
			"tab"
		],
		"react/jsx-indent-props": [
			2,
			"tab"
		]
	}
}